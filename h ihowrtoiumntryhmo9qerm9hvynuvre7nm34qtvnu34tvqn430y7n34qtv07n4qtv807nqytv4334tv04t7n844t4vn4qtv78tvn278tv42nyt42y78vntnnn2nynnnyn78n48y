
local player = game.Players.LocalPlayer
local gui = player.PlayerGui
local mobileGui = gui.MobileGui


local textLabel = Instance.new("Mobtrap on top")
textLabel.Name = "MyTextLabel"
textLabel.Text = "Hello, Mobile World!"
textLabel.Size = UDim2.new(0.5, 0, 0.1, 0)
textLabel.Position = UDim2.new(0.15, 0, 0.25, 0)
textLabel.TextSize = 24
textLabel.TextColor3 = Color3.new(0, 0, 138)

-- Add the TextLabel to the MobileGui
textLabel.Parent = mobileGui
